{
  "name": "svelte-static-template",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "copy:package.json": "cp ./package.json ./src/lib/components/",
    "prebuild": "yarn copy:package.json",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && yarn lint:eslint && yarn lint:stylelint",
    "lint:eslint": "eslint ./src --quiet",
    "lint:stylelint": "stylelint --ignore-path .gitignore ./src/**/*.{scss,svelte} --quiet"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.5",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/eslint": "8.56.7",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.21.0",
    "destyle.css": "^4.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-svelte": "^2.36.0",
    "eslint-plugin-unicorn": "^52.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "postcss-html": "^1.6.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "sass": "^1.74.1",
    "stylelint": "^16.3.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^5.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.9",
    "tslib": "^2.6.2",
    "typescript": "^5.4.4",
    "vite": "^5.2.8"
  },
  "type": "module"
}
