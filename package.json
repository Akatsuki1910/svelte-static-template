{
  "name": "svelte-static-template",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "copy:package.json": "cp ./package.json ./src/lib/components/",
    "prebuild": "yarn copy:package.json",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . --fix && yarn lint:eslint && yarn lint:stylelint",
    "lint:eslint": "eslint ./src --quiet",
    "lint:stylelint": "stylelint --ignore-path .gitignore ./src/**/*.{scss,svelte} --quiet"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.9",
    "@sveltejs/kit": "^2.43.7",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/eslint": "8.56.7",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.21.0",
    "destyle.css": "^4.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-svelte": "^2.46.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "postcss-html": "^1.8.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "sass": "^1.93.2",
    "stylelint": "^16.24.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^6.1.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "svelte": "^4.2.20",
    "svelte-check": "^4.3.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^5.4.11"
  },
  "type": "module"
}
