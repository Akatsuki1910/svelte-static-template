{
  "name": "svelte-static-template",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "copy:package.json": "cp ./package.json ./src/lib/components/",
    "prebuild": "yarn copy:package.json",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && yarn lint:eslint && yarn lint:stylelint",
    "lint:eslint": "eslint ./src --quiet",
    "lint:stylelint": "stylelint --ignore-path .gitignore ./src/**/*.{scss,svelte} --quiet"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.4",
    "@sveltejs/kit": "^2.5.28",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/eslint": "8.56.7",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.21.0",
    "destyle.css": "^4.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-svelte": "^2.44.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "sass": "^1.79.1",
    "stylelint": "^16.9.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^5.1.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "svelte": "^4.2.19",
    "svelte-check": "^4.0.2",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.6"
  },
  "type": "module"
}
